@page "/perfil"
@using MinhasColecoes.Shared.ViewModels;
@inject UsuarioAPI usuarioApi

<h3>Usuario autenticado: @(usuario!= null)</h3>

@code {
	private UsuarioBasicVM usuario;

	protected override async Task OnInitializedAsync()
	{
		if(await usuarioApi.CheckAuthentication())
			usuario = new UsuarioBasicVM();
	}

}
